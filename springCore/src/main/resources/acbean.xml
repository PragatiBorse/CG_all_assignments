<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    
    <bean id="customer" class="com.springCore.Customer">
    <property name="customerId" value="101"></property>
    <property name="customerName" value="101"></property>
    <property name="customerContact" value="101"></property>
    
    <property name="Address" >
    <bean class="com.spring_core.Address">
    <property name="street" value="tikhi"></property>
    <property name="city" value="dhule"></property>
    <property name="state" value="maharashtra"></property>
    <property name="zip" value="424301"></property>
    <property name="country" value="India"></property>
    </bean>
    
    
    
    </property>
    </bean>
    </beans>